{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://ainzlink-backend.onrender.com/api/:path*"
    },
    {
      "source": "/socket.io/:path*",
      "destination": "https://ainzlink-backend.onrender.com/socket.io/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}